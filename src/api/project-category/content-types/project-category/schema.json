{
  "kind": "collectionType",
  "collectionName": "project_categories",
  "info": {
    "singularName": "project-category",
    "pluralName": "project-categories",
    "displayName": "ProjectCategory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "shortDescription": {
      "type": "string",
      "required": true
    },
    "format": {
      "type": "enumeration",
      "enum": [
        "block",
        "inline"
      ]
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "category"
    }
  }
}
